{
  "name": "math-curriculum-app",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "extract": "tsx scripts/extract-curriculum.ts",
    "extract-v2": "tsx scripts/extract-volume2.ts",
    "extract-grade8-v1": "tsx scripts/extract-grade8-volume1.ts",
    "extract-grade8-v2": "tsx scripts/extract-grade8-volume2.ts",
    "migrate": "tsx scripts/migrate-db.ts",
    "seed": "npm run migrate && npm run extract",
    "typecheck": "tsc -p . --noEmit",
    "process-pdfs": "npx tsc scripts/advanced-pdf-extractor.ts --outDir dist --moduleResolution node --target es2020 --module commonjs --esModuleInterop && node scripts/process-all-pdfs.js",
    "test-youtube": "tsx scripts/test-youtube-api.ts",
    "batch-videos": "tsx scripts/batch-video-search.ts"
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "^5.1.0",
    "@azure/core-auth": "^1.10.0",
    "@prisma/client": "^6.15.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/katex": "^0.16.7",
    "@vercel/postgres": "^0.10.0",
    "better-sqlite3": "^12.2.0",
    "framer-motion": "^12.23.12",
    "katex": "^0.16.22",
    "next": "15.5.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-katex": "^3.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "tsx": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
